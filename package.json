{
  "name": "imweb-learning-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host localhost --port 3000",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "seed": "node seed-firebase.mjs",
    "seed:chapters": "node firebase/seed/seedChapters.js",
    "seed:breakdowns": "node firebase/seed/seedBreakdowns.js",
    "update:chapters": "node firebase/seed/updateChapters.js",
    "backfill:slide-counts": "TS_NODE_TRANSPILE_ONLY=1 ts-node firebase/seed/backfillSlideCounts.ts",
    "migrate:practice-sessions": "TS_NODE_TRANSPILE_ONLY=1 ts-node firebase/seed/migratePracticeSessionsFromUserAnswers.ts",
    "dev:create-dummy-progress": "node firebase/seed/devCreateDummyProgress.cjs",
    "emulators:start": "firebase emulators:start",
    "migrate:dry-run": "node scripts/migrate-database.mjs --dry-run",
    "migrate:execute": "node scripts/migrate-database.mjs --migrate"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tabler/icons-react": "^3.34.1",
    "@types/dompurify": "^3.0.5",
    "@types/marked": "^5.0.2",
    "@types/node": "^24.1.0",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.6",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.4.0",
    "framer-motion": "^12.23.12",
    "katex": "^0.16.22",
    "libphonenumber-js": "^1.12.10",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.344.0",
    "marked": "^16.1.2",
    "motion": "^12.23.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-katex": "^3.1.0",
    "react-player": "^3.3.1",
    "react-router-dom": "^7.7.1",
    "tailwind-merge": "^3.3.1",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^30.0.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
